{
  "name": "how7-backend",
  "version": "1.0.0",
  "description": "HOW VII - Backend HTTP/REST para análise de vendas imobiliárias",
  "keywords": [
    "node",
    "express",
    "mysql",
    "rest",
    "analytics"
  ],
  "license": "MIT",
  "author": "HOW VII Team",
  "main": "dist/server.js",
  "scripts": {
    "agent:docs": "node scripts/agent-cmd.js",
    "build": "next build",
    "build:backend": "cd src && tsc --project tsconfig.json",
    "dev": "next dev",
    "dev:backend": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "docs:validate": "node scripts/agent-cmd.js",
    "evidence:generate": "node scripts/generate-evidence.js",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,yml,yaml}\"",
    "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,yml,yaml}\"",
    "format:md": "prettier --write \"**/*.md\"",
    "lint": "next lint",
    "lint:backend": "eslint src/**/*.ts",
    "lint:fix": "next lint --fix",
    "migrate": "npm run build:backend && node EasyImob/dist/db/migrate.js",
    "seed": "npm run build:backend && node EasyImob/dist/db/seed.js",
    "spell:check": "cspell '**/*.{ts,tsx,js,jsx,md,json}'",
    "spell:check-silent": "cspell '**/*.{ts,tsx,js,jsx,md,json}' --silent",
    "start": "next start",
    "start:backend": "node EasyImob/dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "framer-motion": "^12.1.4",
    "geist": "^1.4.2",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.454.0",
    "mysql2": "^3.14.3",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cspell": "^8.8.0",
    "eslint": "^8.57.1",
    "postcss": "^8.5",
    "prettier": "^3.3.0",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}