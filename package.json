{
  "name": "how7-backend",
  "version": "1.0.0",
  "description": "HOW VII - Backend HTTP/REST para análise de vendas imobiliárias",
  "keywords": [
    "node",
    "express",
    "mysql",
    "rest",
    "analytics"
  ],
  "license": "MIT",
  "author": "HOW VII Team",
  "main": "dist/server.js",
  "scripts": {
    "agent:docs": "node scripts/agent-cmd.js",
    "build": "next build",
    "build:backend": "cd src && tsc --project tsconfig.json",
    "dev": "next dev",
    "dev:backend": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "docs:validate": "node scripts/agent-cmd.js",
    "evidence:generate": "node scripts/generate-evidence.js",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,yml,yaml}\"",
    "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,yml,yaml}\"",
    "format:md": "prettier --write \"**/*.md\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:backend": "eslint src/**/*.ts",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "migrate": "npm run build:backend && node EasyImob/dist/db/migrate.js",
    "seed": "npm run build:backend && node EasyImob/dist/db/seed.js",
    "spell:check": "cspell '**/*.{ts,tsx,js,jsx,md,json}'",
    "spell:check-silent": "cspell '**/*.{ts,tsx,js,jsx,md,json}' --silent",
    "start": "next start",
    "start:backend": "node EasyImob/dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit && cd src && tsc --noEmit --project tsconfig.json",
    "type-check:backend": "cd src && tsc --noEmit --project tsconfig.json",
    "type-check:frontend": "tsc --noEmit"
  },
  "dependencies": {
    "@edge-runtime/vm": "latest",
    "@eslint/js": "latest",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-aspect-ratio": "latest",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "latest",
    "@radix-ui/react-collapsible": "latest",
    "@radix-ui/react-context-menu": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "latest",
    "@radix-ui/react-hover-card": "latest",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-menubar": "latest",
    "@radix-ui/react-navigation-menu": "latest",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-radio-group": "latest",
    "@radix-ui/react-scroll-area": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "latest",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-toggle": "latest",
    "@radix-ui/react-toggle-group": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@remix-run/react": "latest",
