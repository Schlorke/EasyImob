{
  "name": "how7-backend",
  "version": "1.0.0",
  "description": "EasyImob Analytics Dashboard - UNIVALI HOW VII Project",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:backend": "tsc --project src/tsconfig.json",
    "dev": "next dev",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "next start",
    "start:backend": "node dist/server.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:backend": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "format:md": "prettier --write **/*.md",
    "format:all": "prettier --write . && eslint --fix .",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "tsc --noEmit",
    "type-check:backend": "tsc --noEmit --project src/tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "spell:check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "spell:add": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --words-only --unique --no-progress | sort -u",
    "agent:docs": "node scripts/agent-cmd.js",
    "docs:validate": "node scripts/agent-cmd.js"
  },
  "dependencies": {
    "@next/font": "^14.0.4",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@types/cors": "^2.8.19",
    "@vercel/analytics": "^1.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "mysql2": "^3.11.3",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@next/eslint-plugin-next": "^15.5.2",
    "@types/express": "^5.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "cspell": "^9.2.1",
    "dotenv": "^17.2.1",
    "eslint": "9.34.0",
    "eslint-config-next": "^15.5.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "recharts": "^3.1.2",
    "supertest": "^7.1.4",
    "tailwindcss": "3.4.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
